{
    "name": "eodhistoricaldata",
    "connection": "api",
    "config": {
        "authentication": "token_parameter",
        "base_url": "https://eodhistoricaldata.com/api",
        "parameter": "api_token",
        "key_vault_secret_name": "eodhistoricaldata-token",
        "output_type": "csv"
    },
    "tables": [
        {
            "name": "AAPL",
            "path": "eod/AAPL.US",
            "date_format": "yyyy-MM-dd",
            "date_from_parameter": "from",
            "parameters": {
                "fmt": "json"
            }
        }
    ]
}